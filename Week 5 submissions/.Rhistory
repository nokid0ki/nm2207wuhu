meme <- function(text, input_image_path, output_image_path) {
img <- image_read("https://i.kym-cdn.com/entries/icons/mobile/000/043/403/cover3.jpg")
text_properties <- image_text(
text,
color = "white",
size = 50,
location = "center",
gravity = "north"
)
img_with_text <- image_composite(img, text_properties, offset = "+0+20")
image_write(img_with_text, path = output_image_path)
}
