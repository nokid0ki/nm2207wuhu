game <- function() {
sentence <- readline(prompt = "Enter a sentence: ")
num_words <- length(strsplit(sentence, " ")[[1]])
num_chars <- nchar(sentence)
avg_word_length <- num_chars / num_words
communication_skill_level <- ifelse(avg_word_length < 4, "Beginner",
ifelse(avg_word_length < 6, "Intermediate", "Advanced"))
cat("Number of words:", num_words, "\n")
cat("Number of characters:", num_chars, "\n")
cat("Average word length:", avg_word_length, "\n")
cat("Communication skill level:", communication_skill_level, "\n")
}
game()
